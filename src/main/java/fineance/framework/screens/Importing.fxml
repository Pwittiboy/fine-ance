                                          </TextField>
                                          <Button fx:id="btnImport" mnemonicParsing="false" text="Upload" />
                                       </children>
                                    </HBox>
                                    <Button fx:id="btnGo" mnemonicParsing="false" onAction="#btnGoAction" prefWidth="80.0" text="Go!" />
                                 </children>
                                 <padding>
                                    <Insets left="30.0" />
